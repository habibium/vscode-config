{
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[swift]": {
    "editor.defaultFormatter": "sswg.swift-lang"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "apisauce",
    "Biriyani",
    "choropleth",
    "cpptools",
    "donot",
    "esbenp",
    "filesize",
    "freecodecamp",
    "habib",
    "habibium",
    "Habibur",
    "headlessui",
    "highlightedyank",
    "lucide",
    "Minerix",
    "nativewind",
    "nowrap",
    "observablehq",
    "opentype",
    "overengineered",
    "Pressable",
    "Pylot",
    "Rahman",
    "scminput",
    "simfree",
    "topojson",
    "unoptimized",
    "vscodevim",
    "webp"
  ],
  "color-highlight.markerType": "dot-after",
  "color-highlight.matchHslWithNoFunction": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "editor.accessibilitySupport": "off",
  "editor.cursorStyle": "block",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'JetBrainsMono Nerd Font', monospace",
  "editor.fontSize": 14,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "material-icon-theme.folders.color": "#f7a96c",
  "material-icon-theme.hidesExplorerArrows": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "/Applications/Ghostty.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.inheritEnv": false,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.highlightedyank.enable": true,
  "vim.leader": "space",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["leader", "a"],
      "after": ["g", "g", "V", "G", "\"", "+", "y"]
    },
    {
      "before": ["leader", "%"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    },
    {
      "before": ["leader", "g", "d"],
      "commands": [
        {
          "command": "editor.action.openLink"
        }
      ]
    },
    { "before": ["K"], "after": ["g", "h"] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "y"],
      "after": ["\"", "+", "y"]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "workbench.colorCustomizations": {
    "editorIndentGuide.activeBackground1": "#fff",
    "editorCursor.background": "#000",
    "editorCursor.foreground": "#fff",
    "gitlens.trailingLineForegroundColor": "#bbbb",
    "tree.inactiveIndentGuidesStroke": "#ffffff44",
    "tree.indentGuidesStroke": "#fff",
    "editorGutter.addedBackground": "#00ff0084",
    "editorGutter.modifiedBackground": "#ffff0084",
    "editorGutter.deletedBackground": "#ff000084"
  },
  "workbench.editor.customLabels.patterns": {
    "**/app/**/**/page.tsx": "${dirname}/",
    "**/**/index.{html,css,js,ts,jsx,tsx,astro}": "${dirname}/"
  },
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "terminal.integrated.enableVisualBell": true,
  "filesize.displayInfoOnTheRightSideOfStatusBar": true,
  "terminal.explorerKind": "external",
  "editor.cursorSurroundingLines": 4,
  "explorer.confirmPasteNative": false,
  "explorer.confirmDragAndDrop": false,
  "reactSnippets.settings.importReactOnTop": false,
  "svgPreview.autoOpen": false,
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "astro"
  ],
  "githubPullRequests.createOnPublishBranch": "never",
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "errorLens.enabledDiagnosticLevels": ["hint"],
  "material-icon-theme.activeIconPack": "react_redux",
  "tailwindCSS.experimental.classRegex": [
    ["className:\\s*\"([^\"]*)\""], // For double quotes
    ["className:\\s*'([^']*)'"], // For single quotes
    ["className:\\s*`([^`]*)`"] // For template literals
  ],
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.iconTheme": "material-icon-theme",
  "files.exclude": {
    "**/.git": false
  },
  "git.autofetch": true,
  "chat.agent.enabled": true,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.copilot",
    "GitHub.copilot-chat",
    "GitHub.vscode-pull-request-github"
  ],
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true
}